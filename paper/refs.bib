	Date-Modified = {2016-08-08 21:12:57 +0000},
	Journal = {Proceedings of the National Academy of Sciences},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {alternative states; ecosystem flips; fisheries collapse; ocean fisheries},
