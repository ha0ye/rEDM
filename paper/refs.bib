@article{Watters_2006,
	Abstract = {Catches per unit of effort (CPUEs) (numbers per 10,000 hooks), of Pacific bluefin tuna (Thunnus orientalis), from the Japanese longline fishery for tunas and billfishes in the Pacific Ocean were analyzed with a regression tree.  A regression tree was fitted to the log-transformed CPUEs (log-CPUEs) achieved by Japanese longliners fishing in the core area defined by Tomlinson (1996) during January 1952-December 1997.  The {``}optimally sized{''} tree had 107 parameters (year, month, latitude, and longitude interacted to affect log-CPUE), explained about 55{\%} of the total variation in log-CPUE, and fitted the data well.  The tree was used to estimate two classes of annual abundance indices for the 58 5$\,^{\circ}$ x 5$\,^{\circ}$ quadrangles in the PBT core area.  {``}Safe{''} indices of abundance were estimated in each quadrangle for only those years in which fishing actually occurred. {``}Extrapolated{''} indices of abundance were estimated for area and times in which fishing did not actually occur. Extrapolated indices should be viewed as hypotheses describing the relative abundance of PBT during times and in locations where fishing did not occur.  The extrapolated indices appeared to be reasonable in many instances.  Two regional indices of abundance were estimated for the entire core area.  These regional indices were estimated with only the safe estimates and with the pooled safe and extrapolated estimates.  Interpretation of the regional time series was fairly robust to whether the extrapolated estimates were included.  During the 1950s and early 1960s, longline CPUEs were high, but variable.  CPUEs declined dramatically during the mid- to late 1960s, and the two regional indices fell from about 1.3 to about 0.4 fish per 10,000 hooks.  During the 1970s CPUEs were relatively stable, and the regional indices varied between about 0.3 and 0.5 fish per 10,000 hooks.  In the early 1980s CPUEs again declined dramatically, and the regional indices fell from about 0.5 to less than 0.2 fish per 10,000 hooks.  There was very little interannual variation in CPUE during most of the 1980s.  Longline CPUEs started to increase in about 1990, and the regional index rose from less than 0.2 to about 0.4-0.5 fish per 10,000 hooks during 1990-1997.
},
	Author = {George Watters},
	Date-Added = {2011-03-02 14:32:01 -0800},
	Date-Modified = {2012-07-20 20:51:40 +0000},
	Local-Url = {file://localhost/Users/hye/Documents/Papers/Watters/Watters_2006_.pdf},
	Pages = {1--10},
	Rating = {0},
	Title = {CPUEs of Pacific Bluefin tuna in a core fishing area extending from the coast of Japan to 150W: using regression trees to hypothesize about CPUEs for times and areas in which there was no fishing},
	Year = {2006},
	Bdsk-File-1 = {}}

